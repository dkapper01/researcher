extends layout

block content
    //h5=title
    .table-responsive
        table.table.table-striped.table-hover.table-bordered.table-margin
            thead
                //tr
                th #
                th Executive
                th 
            tbody
                - var count = 0
                each executive in executive_list
                    tr
                        td(class='count_width') #{++count}
                        td(class='main_width')
                            a(href=executive.url) #{executive.executive_name}
                        td

